<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:keep="@drawable/ic_theme_circle">

    <item>
        <shape android:shape="oval">
            <solid android:color="@color/colorAccent" />
        </shape>
    </item>

    <item>
        <shape android:shape="oval">
            <solid android:color="?attr/BgMtrl" />
        </shape>
    </item>

    <item
        android:bottom="5dp"
        android:drawable="@drawable/ic_theme"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp" />

</layer-list>